#program po uprave
for nasobek in range(5):
    for sloupec in range(5):
        print(sloupec*nasobek, end=' ')
    print(' ')
    
#puvodni program
for nula in range(4):
    print(0, end=' ')
print(0)
for jedna in range(4):
    print(jedna, end=' ')
print(jedna+1)
for dva in range(4):
    print(0 + 2*dva, end=' ')
print(2*4)
for tri in range(4):
    print(0 + 3*tri, end=' ')
print(3*4)
for ctyri in range(4):
    print(0 + 4*ctyri, end=' ')
print(4*4)
